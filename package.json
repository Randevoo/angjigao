  "scripts": {
    "postinstall": "npx tsc",
    "serve": "ts-node-dev -r tsconfig-paths/register -r esm ./src/index.ts",
    "build": "npx tsc",
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/index.js",
    "generateMigration": "dotenv -e .env.dev.local -- npx prisma migrate save --experimental --name",
    "generateMigrationTest": "dotenv -e .env.test -- npx prisma migrate save --experimental --name",
    "migrateDev": "dotenv -e .env.dev.local -- npx prisma migrate up --experimental",
    "migrateTest": "dotenv -e .env.test -- npx prisma migrate up --experimental",
    "codegen": "prisma generate",
    "test": "mocha --exit --timeout 10000 -r ts-node/register -r tsconfig-paths/register -r esm src/__tests__/*.spec.ts src/__tests__/**/*.spec.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
